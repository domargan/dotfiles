[global_config]
  always_split_with_profile = True
  dbus = True
  geometry_hinting = False
  suppress_multiple_term_dialog = True
  tab_position = hidden
  title_hide_sizetext = True
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    font = Monospace 14
    scrollback_infinite = True
    show_titlebar = False
    use_system_font = False
